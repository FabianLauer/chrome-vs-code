{
	"name": "vscode-browser",
	"description": "A web browser for VS Code.",
	"version": "0.0.1",
	"publisher": "Fabian Lauer",
	"engines": {
		"vscode": "^0.10.7"
	},
	"activationEvents": [
		"onCommand:extension.openWebBrowser"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.openWebBrowser",
				"title": "Open Web Browser"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "extension.openWebBrowser"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^2.0.3",
		"vscode": "^0.11.0"
	}
}