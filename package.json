{
  "name": "vscode-browser",
  "description": "A web browser for VS Code.",
  "version": "0.0.1",
  "publisher": "Fabian Lauer",
  "preview": true,
  "engines": {
    "vscode": "^0.10.7"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.openWebBrowser",
        "title": "Open Web Browser"
      },
      {
        "command": "extension.openWebBrowserToSide",
        "title": "Open Web Browser to the Side"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "extension.openWebBrowser"
        },
        {
          "command": "extension.openWebBrowserToSide"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "make && node ./node_modules/vscode/bin/compile",
    "compile": "make && node ./node_modules/vscode/bin/compile -p ./",
    "postinstall": "make && node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "less": "^2.7.1",
    "less-plugin-clean-css": "^1.5.1",
    "prefix-css": "0.0.6",
    "typescript": "^2.0.3",
    "vscode": "^0.11.0"
  },
  "dependencies": {
    "follow-redirects": "^1.0.0",
    "normalize-url": "^1.7.0"
  }
}
